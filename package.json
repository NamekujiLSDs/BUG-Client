{
  "name": "bug-client",
  "productName": "BUG Client",
  "version": "1.0.2",
  "description": "Basic Utility Gear Client for Krunker.",
  "homepage": "https://github.com/NamekujiLSDs/BUG-Client#readme",
  "bugs": {
    "url": "https://github.com/NamekujiLSDs/BUG-Client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NamekujiLSDs/BUG-Client.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": "NamekujiLSDs",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "npx electron .",
    "build": "electron-builder --win --x64 --publish never",
    "publish": "electron-builder --win --x64 --publish always"
  },
  "devDependencies": {
    "electron": "^10.4.7",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "discord-rpc": "^3.2.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^5.4.3",
    "electron-store": "^8.2.0",
    "electron-tabs": "^1.0.4",
    "electron-updater": "^6.1.7",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "v8-compile-cache": "^2.4.0"
  },
  "build": {
    "appId": "basic.utility.gear",
    "directories": {
      "output": "dist"
    },
    "win": {
      "publish": {
        "provider": "github",
        "owner": "NamekujiLSDs"
      },
      "target": "nsis",
      "icon": "/src/assets/img/icons/icon.ico"
    },
    "nsis": {
      "oneClick": true
    }
  }
}